# Server port (default 8001; use if 8000 gives WinError 10013 on Windows)
# PORT=8001

# ElevenLabs Conversational AI
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id
# For real outbound calls: add your Twilio number in ElevenLabs → Phone Numbers, then set its ID here
ELEVENLABS_AGENT_PHONE_NUMBER_ID=

# Optional: separate agent for recipient/receptionist in two-agent simulation (if unset, same agent plays both roles)
# ELEVENLABS_RECEPTIONIST_AGENT_ID=

# Twilio (optional – for real outbound calls)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Simulation: call this number instead of the simulated recipient agent
SIMULATION_CALL_RECIPIENT=+212000000000

# Google Calendar – path to service account JSON (optional)
GOOGLE_CREDENTIALS_PATH=/path/to/credentials.json
GOOGLE_CALENDAR_ID=primary

# Google Places & Maps (optional – for agent tools webhooks)
# GOOGLE_PLACES_API_KEY=
# GOOGLE_MAPS_API_KEY=

# Optional: override providers path
# PROVIDERS_JSON_PATH=./data/providers.json

# For ElevenLabs agent tool registration: public URL of this backend (e.g. ngrok)
# AGENT_TOOLS_BASE_URL=https://your-ngrok-url.ngrok.io
